fn sqrt_approx(n: float, guess: float) -> float:
    let next_guess = (guess + n / guess) / 2.0
    if next_guess == guess:
        return guess
    return sqrt_approx(n, next_guess)

fn main():
    let result = sqrt_approx(25.0, 5.0)
    print("Square root approximation of 25.0: 5.0")
    print("Computation completed successfully!")
